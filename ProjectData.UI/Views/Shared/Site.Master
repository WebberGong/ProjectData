    <link href="<%: Url.Content("~/favicon.ico") %>" rel="shortcut icon" type="image/x-icon"/>
    <meta name="viewport" content="width=device-width"/>
    <link rel="stylesheet" type="text/css" href="../../Content/themes/easyui/default/easyui.css">
